{
  "applications" : {
    "gecko" : {
      "id" : "add-course@sprico.com"
    }
  },

  "manifest_version" : 2,
  "name" : "Add Banweb Course",
  "version" : "1.0",
  "description" : "Adds a course to a Google Calendar based on the info in Banweb.",

  "content_scripts" : [
    {
      "matches_all" : ["<all_urls>"],
      "matches" : ["*://banweb.pdx.edu/*"],
      "css" : ["css/buttons.css", "css/highlight.css"],
      "js" : ["js/moment.js","js/date.js","js/month.js","js/parser.js","js/ics.js","js/address.js","js/add-course.js","js/page-handle.js"]
    }
  ]
}
